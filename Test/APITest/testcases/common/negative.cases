# 用户名包含特殊字符
{"header":{"username":"***","password": "123456","token": "1f364fff-73f5-11f4-8f90- 000f29960ff7","target":null},"body":{}}
{"header":{"status":2,"desc":"fail","failures":[{"code":805,"content":"token格式错误","message":"INVALID_TOKEN"}]}}

# 密码缺失
{"header":{"username":"ShenmaPM2.5","token": "d789a6c8-e6d9-4d7c-bf16-0f518d6888b1","target":null},"body":{}}
{"header":{"status":2,"desc":"fail","failures":[{"code":805,"content":"鉴权参数错误!","message":"invalid_request!"}]}}

# 密码为空
{"header":{"username":"ShenmaPM2.5","password": null,"token": "d789a6c8-e6d9-4d7c-bf16-0f518d6888b1","target":null},"body":{}}
{"header":{"status":2,"desc":"fail","failures":[{"code":805,"content":"鉴权参数错误!","message":"invalid_request!"}]}}

# 密码不匹配
{"header":{"username":"ShenmaPM2.5","password": "123456","token": "d789a6c8-e6d9-4d7c-bf16-0f518d6888b1","target":null},"body":{}}
{"header":{"status":2,"desc":"fail","failures":[{"code":8202,"message":"密码不合法"}]}}
# 密码包含特殊字符
{"header":{"username":"ShenmaPM2.5","password": "@#$%^&*()","token": "d789a6c8-e6d9-4d7c-bf16-0f518d6888b1","target":null},"body":{}}
{"header":{"status":2,"desc":"fail","failures":[{"code":8202,"message":"密码不合法"}]}}
# 密码类型错误：浮点数
{"header":{"username":"ShenmaPM2.5","password": 123.45,"token": "d789a6c8-e6d9-4d7c-bf16-0f518d6888b1","target":null},"body":{}}
{"header":{"status":2,"desc":"fail","failures":[{"code":8202,"message":"密码不合法"}]}}
# 密码类型错误：整数
{"header":{"username":"ShenmaPM2.5","password": 12345,"token": "d789a6c8-e6d9-4d7c-bf16-0f518d6888b1","target":null},"body":{}}
{"header":{"status":2,"desc":"fail","failures":[{"code":8202,"message":"密码不合法"}]}}
# 密码无法正常解析
{"header":{"username":"ShenmaPM2.5 ","password":pd123456,"token":"d789a6c8-e6d9-4d7c-bf16-0f518d6888b1"}}
{"header":{"status":2,"desc":"fail","failures":[{"code":800,"message":"系统错误"}]}}
# Token缺失
{"header":{"username":"ShenmaPM2.5","password":"pd123456"},"body":{}}
{"header":{"status":2,"desc":"fail","failures":[{"code":8401,"message":"缺少token信息"}]}}
# Token为空
{"header":{"username":"ShenmaPM2.5","password":"pd123456","token":null},"body":{}}
{"header":{"status":2,"desc":"fail","failures":[{"code":8401,"message":"缺少token信息"}]}}
# Token类型不符：浮点类型
{"header":{"username":"ShenmaPM2.5","password":"pd123456","token":1234567.890},"body":{}}
{"header":{"status":2,"desc":"fail","failures":[{"code":8402,"message":"token不合法，如需帮助请联系客服顾问"}]}}
# Token类型不符：整型
{"header":{"username":"ShenmaPM2.5","password":"pd123456","token":1234567},"body":{}}
{"header":{"status":2,"desc":"fail","failures":[{"code":8402,"message":"token不合法，如需帮助请联系客服顾问"}]}}

## json值为 '*' 表示可以匹配任意值

# Token类型不符：整型(000000)
{"header":{"username":"ShenmaPM2.5","password":"pd123456","token":000000},"body":{}}
{"header":{"status":2,"desc":"fail","failures":[{"code":800,"message":"系统错误","content":"*"}]}}
# Token包含特殊字符
{"header":{"username":"ShenmaPM2.5","password":"pd123456","token":"@#￥%……&*"},"body":{}}
{"header":{"status":2,"desc":"fail","failures":[{"code":8402,"message":"token不合法，如需帮助请联系客服顾问"}]}}
# Token Json无法正常解析
{"header":{"username":"ShenmaPM2.5","password":"pd123456","token":d789a6c8-e6d9-4d7c-bf16-0f518d6888b1},"body":{}}
{"header":{"status":2,"desc":"fail","failures":[{"code":800,"message":"系统错误"}]}}
# Header多了无效参数：
{"header":{"username":"ShenmaPM2.5","password":"pd123456","token":"d789a6c8-e6d9-4d7c-bf16-0f518d6888b1","INVALID_FOR_TEST":null},"body":{}}
{"header":{"status":2,"desc":"fail","failures":[{"code":800,"message":"系统错误"}]}}
# body多了无效参数
{"header":{"username":"ShenmaPM2.5","password":"pd123456","token":"d789a6c8-e6d9-4d7c-bf16-0f518d6888b1"},"body":{"INVALID_FOR_TEST":null}}
{"header":{"status":2,"desc":"fail","failures":[{"code":800,"message":"系统错误"}]}}