# 用户名非字符串: 数字
{"header":{"username":123456,"password": "123456","token": "1f364fff-73f5-11f4-8f90- 000f29960ff7","target":null},"body":{}}
{"header":{"status":2,"desc":"fail","failures":[{"code":800,"message":"系统错误"}]}}  

# 用户名非字符串: 浮点数
{"header":{"username":1234.56,"password": "123456","token": "1f364fff-73f5-11f4-8f90- 000f29960ff7","target":null},"body":{}}
{"header":{"status":2,"desc":"fail","failures":[{"code":800,"message":"系统错误"}]}} 

# 用户名为空
{"header":{"username":null,"password": "123456","token": "1f364fff-73f5-11f4-8f90- 000f29960ff7","target":null},"body":{}}
{"header":{"status":2,"desc":"fail","failures":[{"code":805,"content":"鉴权参数错误!","message":"invalid_request!"}]}}

# 用户名不存在
{"header":{"username":"123456","password": "123456","token": "1f364fff-73f5-11f4-8f90- 000f29960ff7","target":null},"body":{}}
{"header":{"status":2,"desc":"fail","failures":[{"code":805,"content":"token格式错误","message":"INVALID_TOKEN"}]}}

